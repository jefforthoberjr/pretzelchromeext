{
  "manifest_version": 3,
  "name": "Word Grenade",
  "description": "Blow up words on the page!",
  "version": "1.63",
   "icons": {
    "16": "images/pretzel_grenade_16.png",
    "32": "images/pretzel_grenade_32.png",
    "48": "images/pretzel_grenade_48.png",
    "128": "images/pretzel_grenade_128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/pretzel_grenade.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}